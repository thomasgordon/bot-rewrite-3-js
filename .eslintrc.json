{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["eslint:all"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "camelcase": [
      "error",
      {
        "properties": "never"
      }
    ],
    "dot-location": ["error", "property"],
    "func-style": "off",
    "function-call-argument-newline": ["warn", "consistent"],
    "function-paren-newline": "off",
    "id-length": "off",
    "implicit-arrow-linebreak": "off",
    "indent": ["warn", 2],
    "max-lines-per-function": ["warn", 200],
    "max-statements": ["warn", 30],
    "newline-per-chained-call": "off",
    "no-await-in-loop": "off",
    "no-console": "off",
    "no-inline-comments": "off",
    "no-magic-numbers": "off",
    "no-mixed-operators": [
      "error",
      {
        "groups": [
          ["&", "|", "^", "~", "<<", ">>", ">>>"],
          ["&&", "||"]
        ]
      }
    ],
    "no-plusplus": "off",
    "no-ternary": "off",
    "no-undefined": "off",
    "object-curly-spacing": "off",
    "one-var": ["warn", "never"],
    "padded-blocks": "off",
    "quote-props": "off",
    "quotes": ["warn", "double"],
    "require-atomic-updates": "warn",
    "require-await": "off",
    "semi": ["warn", "always"],
    "space-before-function-paren": "off"
  }
}
