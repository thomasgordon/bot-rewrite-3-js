{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["warn", 2],
    "linebreak-style": ["off"],
    "no-unused-vars": ["off"],
    "quotes": ["warn", "double"],
    "semi": ["warn", "always"]
  }
}
