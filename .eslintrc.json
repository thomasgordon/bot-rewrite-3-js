{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:all",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": ["warn", 2],
    "quotes": ["warn", "double"],
    "semi": ["warn", "always"],
    "one-var": ["warn", "never"],
    "no-magic-numbers": "off",
    "no-console": "off",
    "no-inline-comments": "off"
  }
}
